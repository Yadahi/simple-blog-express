<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />
    <title><%= pageTitle %></title>
  </head>
  <body>
    <%- include('../includes/navigation.ejs') %>
    <div class="container">
      <h1>My Blog</h1>
      <div class="grid">
        <% articles.forEach(function(article) { %>
        <div class="article">
          <h2><%= article.title %></h2>
          <p><%= article.content %></p>
        </div>
        <% }); %>
      </div>
    </div>
  </body>
</html>
